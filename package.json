{
  "scripts": {
    "build": "npx tsc && npx esbuild --bundle ./src/cli.ts --platform=node --outdir=dist --sourcemap --format=esm --out-extension:.js=.mjs --external:sqlite3",
    "migrate": "npx ts-node --esm ./src/cli.ts migrate",
    "reset-db": "rm -f ./database.db && npx yarn migrate",
    "serve": "ts-node --esm ./src/cli.ts serve",
    "lint": "npx eslint ./src/**/*.ts",
    "lint-fix": "npx eslint --fix ./src/**/*.ts",
    "test": "npx jest"
  },
  "dependencies": {
    "@aave/core-v3": "^1.18.0",
    "@types/express": "^4.17.17",
    "@types/pug": "^2.0.6",
    "better-sqlite-pool": "^0.3.2",
    "better-sqlite3": "^8.5.0",
    "click": "^0.1.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "pug": "^3.0.2",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "*",
    "yarn": "^1.22.19"
  },
  "type": "module",
  "license": "MIT"
}
